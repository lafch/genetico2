package net.uch.util;

/**
 *
 * @author biblioteca
 */
public class BeanParametros {
    private String proNombre;
    private Object proValor;
    private String proOperador;

    public BeanParametros(String proNombre, Object proValor,String proOperador) {
        this.proNombre = proNombre;
        this.proValor = proValor;
        this.proOperador=proOperador;
    }

    
    public String getProNombre() {
        return proNombre;
    }

    public void setProNombre(String proNombre) {
        this.proNombre = proNombre;
    }

    public Object getProValor() {
        return proValor;
    }

    public void setProValor(Object proValor) {
        this.proValor = proValor;
    }

    public String getProOperador() {
        return proOperador;
    }

    public void setProOperador(String proOperador) {
        this.proOperador = proOperador;
    }
    
    
}
