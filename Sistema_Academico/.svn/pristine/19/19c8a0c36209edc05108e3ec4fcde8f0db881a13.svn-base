@charset "utf-8";
/* CSS Document */

body {
    font-size: 10px;
}

td {
    font-size:10px;
}

table {
    font-size: 10px;
}

form fieldset legend {
    font-family:Verdana, Arial, Helvetica, sans-serif;
    color:#000000;
    font-weight:bold;
    font-variant:small-caps;
    font-size:110%;

    padding:2px 15px;
    margin:0px 0px 10px 0px;
    position:relative;
    top: -12px;
}

form fieldset legend img {
    padding:0px 5px 0px 5px;
}

.button {
    background-color:#d3effe;
    font-size: 10px;
    border-width:1px;
    border-style:solid;
    border-color:#000000;
    color:#000000;
    font-weight:bold;
    font-family:Verdana, Arial, Helvetica, sans-serif;
}

input {
    font-size: 10px;
}

select {
    font-size:10px;
}

.tdLabel {
    font-weight: bold;
    padding: 0 10px;
    text-align: right;
}

.tdLabel2 {
    font-weight: bold;
    padding: 0 10px;
    text-align: right;
    width: 80px;
}

.message {
    font-size: 13px;
    font-weight: bold;
    padding: 0;
}

.error {
    color: #C0402A;
    text-align: center;
}

.success {
    color: #339900;
    text-align: center;
}

.rich-fileupload-list-decor {
    height: 96px;
    width: 347px
}

.rich-message-marker img {
    padding-right:7px;
}

.rich-message-label {
    color:red;
    font-size:7pt;
}
