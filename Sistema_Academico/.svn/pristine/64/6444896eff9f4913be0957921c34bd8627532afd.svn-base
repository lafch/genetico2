<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="net.uch.mapping">
	<class
		name="TbBloqueoWeb"
		table="tb_bloqueo_web"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="bloweb_id"
			type="integer"
			column="bloweb_id"
		>
			<generator class="native"/>
		</id>

		<property
			name="sec_id"
			column="sec_id"
			type="integer"
			not-null="false"
			length="11"
		/>
		<property
			name="bloweb_estado"
			column="bloweb_estado"
			type="string"
			not-null="false"
			length="6"
		/>
		<property
			name="bloweb_activo"
			column="bloweb_activo"
			type="string"
			not-null="false"
			length="1"
		/>


	</class>	
</hibernate-mapping>