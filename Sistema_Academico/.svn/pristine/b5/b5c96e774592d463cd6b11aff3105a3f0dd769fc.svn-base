package net.uch.mapping.base;

import net.uch.mapping.AcAlumno;
import net.uch.mapping.AcAlumnoEstado;
import net.uch.mapping.AcSemestre;

/**
 *
 * @author richard
 */
public class BaseAcEstadoSemestre {
    private AcAlumno acAlumno;
    private AcSemestre acSemestre;
    private AcAlumnoEstado acAlumnoEstado;
    private Integer estsemId;

    public BaseAcEstadoSemestre(Integer estsemId) {
        this.estsemId = estsemId;
    }
    
    public BaseAcEstadoSemestre() {
    }

    public BaseAcEstadoSemestre(AcAlumno acAlumno, AcSemestre acSemestre, AcAlumnoEstado acAlumnoEstado, Integer estsemId) {
        this.acAlumno = acAlumno;
        this.acSemestre = acSemestre;
        this.acAlumnoEstado = acAlumnoEstado;
        this.estsemId = estsemId;
    }
    
    

    public AcAlumno getAcAlumno() {
        return acAlumno;
    }

    public void setAcAlumno(AcAlumno acAlumno) {
        this.acAlumno = acAlumno;
    }

    public AcAlumnoEstado getAcAlumnoEstado() {
        return acAlumnoEstado;
    }

    public void setAcAlumnoEstado(AcAlumnoEstado acAlumnoEstado) {
        this.acAlumnoEstado = acAlumnoEstado;
    }

    public AcSemestre getAcSemestre() {
        return acSemestre;
    }

    public void setAcSemestre(AcSemestre acSemestre) {
        this.acSemestre = acSemestre;
    }

    public Integer getEstsemId() {
        return estsemId;
    }

    public void setEstsemId(Integer estsemId) {
        this.estsemId = estsemId;
    }
    
    
}
